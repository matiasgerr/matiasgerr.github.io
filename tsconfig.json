{
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "types": ["astro/client"],
    "strict": true,
    "skipLibCheck": true
  }
}